public class main{

  public static void main(String[] args) {

    int num = 29;
    boolean flag = false;
    if (num == 0 || num == 1) {
        flag = true;
    }
    if (!flag)
      System.out.println(num + " is a prime number.");
    else
      System.out.println(num + " is not a prime number.");
  }
}

