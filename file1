
public class Main {

  public static void main(String[] args)
    int num = 29;
    boolean flag = false;

    // 0 and 1 are not prime numbers
    if (num == 0 || num == 1) {
        flag = true;
   }
   if (!flag)
      System.out.println(num + " is prime);
   else
      System.out.println(num + " is  not prime number);
  }
}

